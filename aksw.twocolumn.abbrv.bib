  Keywords                 = {2013 group_aksw event_wise khalili auer rdface sys:relevantFor:infai sys:relevantFor:bis sys:relevantFor:lod2 peer-reviewed},
  Timestamp                = {2013-07-18T11:27:45.000+0200},
  Url                      = {http://svn.aksw.org/papers/2013/WISE_RDFaCE/public.pdf}
}

