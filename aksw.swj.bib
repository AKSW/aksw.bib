@InCollection{LOD2Book-LODCycleToolstack,
  Title                    = {Supporting the {L}inked {D}ata Life Cycle Using an Integrated Tool Stack},
  Author                   = {Bert Van Nuffelen and Valentina Janev and Michael Martin and Vuk Mijovic and Sebastian Tramp},
  Booktitle                = {Linked Open Data---Creating Knowledge Out of Interlinked Data},
