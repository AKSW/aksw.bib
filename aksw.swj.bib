@InProceedings{riechert-t-2010-iswc,
  Title                    = {Knowledge Engineering for Historians on the Example of the Catalogus Professorum Lipsiensis},
  Author                   = {Thomas Riechert and Ulf Morgenstern and S{\"o}ren Auer and Sebastian Tramp and Michael Martin},
  Booktitle                = {Proceedings of the 9th International Semantic Web Conference (ISWC2010)},
  Year                     = {2010},

  Address                  = {Shanghai / China},
