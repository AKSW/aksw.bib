@InProceedings{frischmuth2103a,
  Title                    = {Towards {L}inked {D}ata based Enterprise Information Integration},
  Author                   = {Philipp Frischmuth and Sören Auer and Sebastian Tramp and Jörg Unbehauen and Kai Holzweißig and Car-Martin Marquardt},
